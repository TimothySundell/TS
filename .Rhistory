advanced_export_plots <- function(seurat_object, file_prefix, feature_list, pt.size = 1.5, order = TRUE, cols = myPalette) {
for(i in 1:length(feature_list)) {
temp_plot <- FeaturePlot(object = seurat_object, reduction = "tsne", order = order, cols = cols, pt.size = pt.size, features = feature_list[i])
ggsave(filename = paste(file_prefix,"_", feature_list[i], ".pdf", sep = ""), plot = temp_plot, units = "in", width = 11.69, height = 8.27)
print(paste("Exported figure for", feature_list[i]))
}
print(paste("Finished exporting", i, "figure(s)"))
}
View(advanced_export_plots)
